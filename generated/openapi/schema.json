{
  "info": {
    "license": {
      "name": ""
    },
    "title": "Problem and Submission API",
    "version": "0.1.0"
  },
  "openapi": "3.1.0",
  "components": {
    "schemas": {
      "Language": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "version": {
            "type": "string"
          }
        },
        "required": [
          "name",
          "version"
        ]
      },
      "TestCase": {
        "type": "object",
        "properties": {
          "input": {
            "type": "string"
          },
          "output": {
            "type": "string"
          }
        },
        "required": [
          "input",
          "output"
        ]
      },
      "Problem": {
        "type": "object",
        "properties": {
          "body": {
            "type": "string"
          },
          "id": {
            "type": "integer",
            "minimum": 0
          },
          "supported_languages": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Language"
            }
          },
          "test_cases": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TestCase"
            }
          },
          "title": {
            "type": "string"
          }
        },
        "required": [
          "body",
          "id",
          "supported_languages",
          "test_cases",
          "title"
        ]
      },
      "ProblemCreate": {
        "type": "object",
        "properties": {
          "body": {
            "type": "string"
          },
          "supported_languages": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Language"
            }
          },
          "test_cases": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TestCase"
            }
          },
          "title": {
            "type": "string"
          }
        },
        "required": [
          "body",
          "supported_languages",
          "test_cases",
          "title"
        ]
      },
      "ProblemUpdate": {
        "type": "object",
        "properties": {
          "body": {
            "type": "string"
          },
          "supported_languages": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Language"
            }
          },
          "test_cases": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TestCase"
            }
          },
          "title": {
            "type": "string"
          }
        },
        "required": [
          "body",
          "supported_languages",
          "test_cases",
          "title"
        ]
      },
      "SubmissionStatus": {
        "type": "string",
        "enum": [
          "Accepted",
          "WrongAnswer",
          "RuntimeError",
          "CompileError"
        ]
      },
      "SubmissionResult": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          },
          "status": {
            "$ref": "#/components/schemas/SubmissionStatus"
          }
        },
        "required": [
          "status"
        ]
      },
      "TestStatus": {
        "type": "string",
        "enum": [
          "Passed",
          "Failed"
        ]
      },
      "TestResult": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          },
          "status": {
            "$ref": "#/components/schemas/TestStatus"
          },
          "test_case_id": {
            "type": "integer",
            "minimum": 0
          }
        },
        "required": [
          "status",
          "test_case_id"
        ]
      },
      "Submission": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string"
          },
          "id": {
            "type": "integer",
            "minimum": 0
          },
          "language": {
            "$ref": "#/components/schemas/Language"
          },
          "problem_id": {
            "type": "integer",
            "minimum": 0
          },
          "result": {
            "$ref": "#/components/schemas/SubmissionResult"
          },
          "student_id": {
            "type": "integer",
            "minimum": 0
          },
          "submitted_at": {
            "type": "string",
            "format": "date-time"
          },
          "test_results": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TestResult"
            }
          }
        },
        "required": [
          "code",
          "id",
          "language",
          "problem_id",
          "result",
          "student_id",
          "submitted_at",
          "test_results"
        ]
      },
      "SubmissionCreate": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string"
          },
          "language": {
            "$ref": "#/components/schemas/Language"
          }
        },
        "required": [
          "code",
          "language"
        ]
      },
      "AuthenticationChallenge": {
        "type": "object",
        "properties": {
          "challenge": {
            "type": "string"
          }
        },
        "required": [
          "challenge"
        ]
      },
      "UserRegistration": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "format": "email"
          },
          "name": {
            "type": "string"
          },
          "role": {
            "type": "string",
            "enum": [
              "admin",
              "teacher",
              "student"
            ]
          }
        },
        "required": [
          "email",
          "name",
          "role"
        ]
      },
      "AuthenticationResponse": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string"
          }
        },
        "required": [
          "token"
        ]
      },
      "Credential": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "rawId": {
            "type": "string"
          },
          "type": {
            "type": "string",
            "enum": [
              "public-key"
            ]
          }
        },
        "required": [
          "id",
          "rawId",
          "type"
        ]
      },
      "RegistrationCredential": {
        "allOf": [
          {
            "$ref": "#/components/schemas/Credential"
          },
          {
            "type": "object",
            "properties": {
              "response": {
                "type": "object",
                "properties": {
                  "attestationObject": {
                    "type": "string"
                  },
                  "clientDataJSON": {
                    "type": "string"
                  }
                },
                "required": [
                  "attestationObject",
                  "clientDataJSON"
                ]
              }
            },
            "required": [
              "response"
            ]
          }
        ]
      },
      "AuthenticationCredential": {
        "allOf": [
          {
            "$ref": "#/components/schemas/Credential"
          },
          {
            "type": "object",
            "properties": {
              "response": {
                "type": "object",
                "properties": {
                  "authenticatorData": {
                    "type": "string"
                  },
                  "clientDataJSON": {
                    "type": "string"
                  },
                  "signature": {
                    "type": "string"
                  },
                  "userHandle": {
                    "type": "string"
                  }
                },
                "required": [
                  "authenticatorData",
                  "clientDataJSON",
                  "signature"
                ]
              }
            },
            "required": [
              "response"
            ]
          }
        ]
      }
    },
    "parameters": {}
  },
  "paths": {
    "/api/problems": {
      "get": {
        "operationId": "getProblems",
        "summary": "問題の一覧を取得する",
        "tags": [
          "problems"
        ],
        "responses": {
          "200": {
            "description": "問題の一覧",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Problem"
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "operationId": "createProblem",
        "summary": "新しい問題を作成する",
        "tags": [
          "problems"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ProblemCreate"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "作成された問題",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Problem"
                }
              }
            }
          }
        }
      }
    },
    "/api/problems/{problemId}": {
      "get": {
        "operationId": "getProblemById",
        "summary": "問題の詳細を取得する",
        "tags": [
          "problems"
        ],
        "parameters": [
          {
            "schema": {
              "type": "integer",
              "example": "1"
            },
            "required": true,
            "in": "path",
            "name": "problemId"
          }
        ],
        "responses": {
          "200": {
            "description": "問題の詳細",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Problem"
                }
              }
            }
          },
          "404": {
            "description": "指定されたIDの問題が見つかりません"
          }
        }
      },
      "put": {
        "operationId": "updateProblem",
        "summary": "問題を更新する",
        "tags": [
          "problems"
        ],
        "parameters": [
          {
            "schema": {
              "type": "integer",
              "example": "1"
            },
            "required": true,
            "in": "path",
            "name": "problemId"
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ProblemUpdate"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "更新された問題",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Problem"
                }
              }
            }
          },
          "404": {
            "description": "指定されたIDの問題が見つかりません"
          }
        }
      },
      "delete": {
        "operationId": "deleteProblem",
        "summary": "問題を削除する",
        "tags": [
          "problems"
        ],
        "parameters": [
          {
            "schema": {
              "type": "integer",
              "example": "1"
            },
            "required": true,
            "in": "path",
            "name": "problemId"
          }
        ],
        "responses": {
          "204": {
            "description": "問題が削除されました"
          },
          "404": {
            "description": "指定されたIDの問題が見つかりません"
          }
        }
      }
    },
    "/api/problems/{problemId}/submit": {
      "post": {
        "operationId": "submitProgram",
        "summary": "問題に対してプログラムを提出する",
        "tags": [
          "problems"
        ],
        "parameters": [
          {
            "schema": {
              "type": "integer",
              "example": "1"
            },
            "required": true,
            "in": "path",
            "name": "problemId"
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SubmissionCreate"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "提出されたプログラム",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Submission"
                }
              }
            }
          },
          "400": {
            "description": "提出データが不正です"
          },
          "404": {
            "description": "指定されたIDの問題が見つかりません"
          }
        }
      }
    },
    "/api/problems/{problemId}/test": {
      "post": {
        "operationId": "testProgram",
        "summary": "問題に対してプログラムをテストする",
        "tags": [
          "problems"
        ],
        "parameters": [
          {
            "schema": {
              "type": "integer",
              "example": "1"
            },
            "required": true,
            "in": "path",
            "name": "problemId"
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SubmissionCreate"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "提出されたプログラムのテスト結果",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/TestResult"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/problems/{problemId}/submissions": {
      "get": {
        "operationId": "getSubmissionsByProblemId",
        "summary": "問題に対する提出一覧を取得する",
        "tags": [
          "problems"
        ],
        "parameters": [
          {
            "schema": {
              "type": "integer",
              "example": "1"
            },
            "required": true,
            "in": "path",
            "name": "problemId"
          }
        ],
        "responses": {
          "200": {
            "description": "提出一覧",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Submission"
                  }
                }
              }
            }
          },
          "404": {
            "description": "指定されたIDの問題が見つかりません"
          }
        }
      }
    },
    "/api/submissions": {
      "get": {
        "operationId": "getSubmissions",
        "summary": "提出一覧を取得する",
        "tags": [
          "submissions"
        ],
        "responses": {
          "200": {
            "description": "提出一覧",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Submission"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/submissions/{submissionId}": {
      "get": {
        "operationId": "getSubmissionById",
        "summary": "提出の詳細を取得する",
        "tags": [
          "submissions"
        ],
        "parameters": [
          {
            "schema": {
              "type": "integer",
              "example": "1"
            },
            "required": true,
            "in": "path",
            "name": "submissionId"
          }
        ],
        "responses": {
          "200": {
            "description": "提出の詳細",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Submission"
                }
              }
            }
          },
          "404": {
            "description": "指定されたIDの提出が見つかりません"
          }
        }
      }
    },
    "/api/register": {
      "post": {
        "summary": "ユーザー登録の開始",
        "tags": [
          "auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UserRegistration"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "登録チャレンジの生成",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AuthenticationChallenge"
                }
              }
            }
          },
          "400": {
            "description": "無効なリクエストまたは既に登録済み"
          }
        }
      }
    },
    "/api/register/verify": {
      "post": {
        "summary": "登録の検証とトークン発行",
        "tags": [
          "auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RegistrationCredential"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "登録成功とトークン発行",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AuthenticationResponse"
                }
              }
            }
          },
          "400": {
            "description": "検証失敗"
          }
        }
      }
    },
    "/api/authenticate": {
      "post": {
        "summary": "認証の開始",
        "tags": [
          "auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string",
                    "format": "email"
                  }
                },
                "required": [
                  "email"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "認証チャレンジの生成",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AuthenticationChallenge"
                }
              }
            }
          },
          "400": {
            "description": "認証情報が登録されていません"
          },
          "404": {
            "description": "ユーザーが見つかりません"
          }
        }
      }
    },
    "/api/authenticate/verify": {
      "post": {
        "summary": "認証の検証とトークン発行",
        "tags": [
          "auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AuthenticationCredential"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "認証成功とトークン発行",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AuthenticationResponse"
                }
              }
            }
          },
          "401": {
            "description": "認証失敗"
          }
        }
      }
    }
  },
  "webhooks": {}
}